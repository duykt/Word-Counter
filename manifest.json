{
    "manifest_version": 2,
    "name": "Word Counter",
    "version": "1.0",
    "description": "A browser extention that displays word count of highlighted text",
    "permissions": ["activeTab, scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}